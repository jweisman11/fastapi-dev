from pydantic import BaseModel
from enum import Enum

class DhsComponentEnum(str, Enum):
    CBP = 'CBP'
    ICE = 'ICE'
    USCG = "USCG"

class BabyDataset(BaseModel):
    """
    Scheme for a data
    """
    id: int
    name: str
    owner: DhsComponentEnum.spanner


class BabyDatasetCreate(BaseModel):
    """
    Schema for creating new dataset
    """
    name: str
    owner: DhsComponentEnum.spanner
